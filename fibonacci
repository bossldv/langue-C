#include <stdio.h>
#include <math.h>
int main()
   {
   int f1,f2,t,fib,nom,flag;
        f1=0; 
        f2=1;
        nom=1;
        flag=0;
        scanf("\n%d", &fib);
        while (flag!=1)
        {
        t=f1+f2;
        f1=f2;
        f2=t;
        nom+=1;
        if (f2==fib) 
        {
        printf("\n%d", nom);
        flag=1;
        }
        else if (f2>fib)
        {
        printf("-1\n");
        flag=1;
        }
                          
          };
         
         
               }
